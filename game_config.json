{
  "game_name": "TankMan",
  "version": "1.0.0",
  "url": "https://github.com/Jesse-Jumbo/your repositoriy name",
  "game_params": [
    {
      "name": "map_no",
      "verbose": "地圖編號",
      "type": "int",
      "max": 2,
      "min": 1,
      "default": 1,
      "help": "set map number for load game scene"
    },
    {
      "name": "time_limit",
      "verbose": "遊戲時間(s)",
      "type": "int",
      "max": 600,
      "min": 1,
      "default": 30,
      "help": "set the time number of game time limit."
    },
    {
      "name": "sound",
      "verbose": "遊戲音效",
      "type": "str",
      "choices": [
        "on",
        "off"
      ],
      "help": "'off' can turn off the sound.",
      "default": "on"
    }
  ]
}
