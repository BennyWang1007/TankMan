{
  "game_name": "TankMan",
  "version": "0.0.7",
  "url": "https://github.com/Jesse-Jumbo/TankMan",
  "description": "坦克人(TankMan)，一款經典的雙人對戰遊戲，時間內分數高者獲勝，除了擊中對手外，破壞遊戲物件，以獲得更高積分。注意: 場上資源恢復皆須時間，先到者得，你需要這些補充品以提供前進和射擊的燃油和子彈。",
  "logo": [
    "./asset/image/player_1P.png",
    "https://github.com/Jesse-Jumbo/TankMan/blob/main/asset/image/player_1P.png"
  ],
  "game_params": [
    {
      "name": "map_no",
      "verbose": "地圖編號",
      "type": "int",
      "max": 3,
      "min": 1,
      "default": 1,
      "help": "set map number for load game scene"
    },
    {
      "name": "time_limit",
      "verbose": "遊戲時間(s)",
      "type": "int",
      "max": 600,
      "min": 1,
      "default": 30,
      "help": "set the time number of game time limit."
    },
    {
      "name": "sound",
      "verbose": "遊戲音效",
      "type": "str",
      "choices": [
        "on",
        "off"
      ],
      "help": "'off' can turn off the sound.",
      "default": "on"
    }
  ]
}
